-none position-relative\"\n  }, __jsx(Link, {\n    href: \"/other/cart\"\n  }, __jsx(\"a\", {\n    className: \"nav-link mini-cart-trigger pr-3 pr-lg-0\"\n  }, __jsx(AiOutlineShoppingCart, null), cartItems.length > 0 ? __jsx(\"span\", {\n    className: \"cart-count cart-count--mobile\"\n  }, cartItems.length) : \"\"))), __jsx(\"li\", {\n    className: \"d-block d-lg-none\"\n  }, __jsx(\"button\", {\n    className: \"nav-link mobile-menu-trigger pr-0\",\n    onClick: () => {\n      setOffCanvasMobileMenuActive(true);\n    }\n  }, __jsx(IoIosMenu, null))))))), __jsx(SearchOverlay, {\n    activeStatus: offCanvasSearchActive,\n    getActiveStatus: setOffCanvasSearchActive\n  }), __jsx(MobileMenu, {\n    activeStatus: offCanvasMobileMenuActive,\n    getActiveStatus: setOffCanvasMobileMenuActive,\n    productData: productData\n  }));\n};\n\nconst mapStateToProps = state => {\n  return {\n    cartItems: state.cartData\n  };\n};\n\nexport default connect(mapStateToProps)(SubHeader);","map":null,"metadata":{},"sourceType":"module"}